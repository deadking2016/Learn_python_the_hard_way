people = 30
cats = 30
dogs = 15

if people < cats:
	print "Too many cats"

if people > cats:
	print "Not many cats"

if people < dogs:
	print " The world is drooled"

if people > dogs:
	print " The world is dry"

dogs += 5

if people >= dogs:
	print "People are greater or equal"

if people <= dogs:
	print "People are less or equal"

if people == dogs:
	print "People are dogs"
